programa {
  funcao inicio() {
    inteiro valor1=0,valor2=0,valor3=0,total=0
    escreva("Digite um número: ")
    leia(valor1)
    escreva("Digite outro número: ")
    leia(valor2)
    escreva("Digite outro número: ")
    leia(valor3)
    total = (valor1+valor2+valor3)/3
    escreva("A média dos números é: "+total)
  }
}
